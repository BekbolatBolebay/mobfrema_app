{
  "version": 2,
  "routes": [
    {
      "src": "/api/paybox/(.*)",
      "dest": "/api/paybox/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_PAYBOX_MERCHANT_ID": "@paybox_merchant_id",
    "PAYBOX_SECRET_KEY": "@paybox_secret_key",
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_anon_key"
  },
  "regions": ["fra1", "arn1"]
}
